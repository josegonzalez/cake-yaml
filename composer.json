{
    "name": "makallio85/cake-yaml",
    "description": "cake-yaml plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeYaml\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev"
}
